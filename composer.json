{
  "name": "swoft/testlib",
  "type": "library",
  "keywords": [
    "swoole",
    "swoft",
    "swoft-component"
  ],
  "description": "testlib component for swoft framework",
  "license": "Apache-2.0",
  "require": {
    "php": ">7.1"
  },
  "autoload": {
    "psr-4": {
      "Swoft\\TestLib\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload-dev": {
    "psr-4": {
      "SwoftTest\\TestLib\\Unit\\": "test/unit/",
      "SwoftTest\\TestLib\\Testing\\": "test/testing/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
